auto_activate_base: true
auto_update_conda: false
channels:
  - conda-forge
  - defaults
  - plotly
  - r
create_default_packages:
  - flask
  - ipykernel
  - ipython
  - ipywidgets
  - jupyterlab
  - jupyter-dash
  - matplotlib
  - numpy
  - numpy-financial
  - pandas
  - pandoc
  - pip
  - plotly
  - scipy
show_channel_urls: true
verbosity: 1
pip_interop_enabled: true
